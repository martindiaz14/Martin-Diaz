body{
    background: linear-gradient(150deg, #50379B ,#b736b9);
    height: 98vh; 
    background-repeat: no-repeat;
    background-attachment: fixed;
    margin-right: auto;
}


/*Diseño general de botones*/
button:hover{
    background-color:rgb(114, 63, 161);
    transition: 0.5s;
}
button{
    background-color: rgb(119, 30, 146);
    color: rgb(255, 255, 255);
    box-shadow: 3px 3px 0px #000;
    border: 0px;
}
.btn-inicio{
    padding:15px 35px;
    margin-top: 10px;
    border-radius: 5px; 
    font-weight: 800px;
}
button a{
    color: #ffffff;
    text-decoration: none;
}

.btn-bck{   
    padding:15px 35px;
    border: 0px;
    margin-top: 10px;
    border-radius: 5px; 
    font-weight: 500;
    text-decoration: none;
}
.btn-bck:hover{
    background-color:rgb(114, 63, 161);
    transition: 0.5s;
}
.btn-bck a{
    color: #ffffff;
    text-decoration: none;
}
.btnlogout{
    padding: 10px 30px;
    border: 0px;
    margin-top: 10px;
    border-radius: 5px; 
    font-weight: 800px;
    margin-left: 20px;
}
#remove{
    margin: 10px;
    background-color: #636262;
    border-radius: 5px;
    padding: 10px;
}
#remove:hover{

    background-color: #474747;

}
/*Diseño general de botones*/
