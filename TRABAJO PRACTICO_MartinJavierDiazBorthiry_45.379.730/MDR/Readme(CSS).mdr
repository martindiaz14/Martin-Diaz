/*Diseño de HOME*/
.barra{
    width: 100%;
    justify-content: space-between;
    background-color: #000;
    color: #ffffff;
    position:fixed;
    height: 100px;
    top: 0;
    right: 0;    
    box-shadow: #7a1f91 1px 5px 0px ; 

}
.barra a{
    font-weight: 600;
    color: aliceblue;
    text-decoration:none;
    font-size: 16px;
    margin: 5px;
    font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
}
.barra ul{
    margin-top: -70px;
    line-height: 50px;   
    float: right;   
    margin-right: -20px;
    width: 45%;
    display:flex inline-grid;
}

.barra a:hover{
    color: #a77ccf;
    transition: 0.5s;
}

.categorias{
    color: azure;
    background-color: aqua;
}   
#icono{
    float: left ;  
    display:flex;
}
#icono:hover{
    background-color: #2d1544;
    border-radius: 80px;
    transition:0.5s;
}

#tit{
    width: 300px;
    margin-top: -20px;   
    font-size: 30px;
    color: #ffffff;
    text-shadow: #aa31c2 3px 1px 0px;
    margin-left: 10px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;    
}
/*Diseño de HOME*/
/*Diseño de Cards*/
