/*Diseño de El titulo de Inicio*/
.Container{
    align-items: center;
    background-color: rgb(38, 11, 56);
    
    border: 20px solid rgb(63, 10, 87);
    width: 96%;
    padding-bottom: 20px;
    color: rgb(255, 255, 255);
    font-style: oblique;
    border-radius: 10px;
    text-align: center;
    font-size: 20px;
    box-shadow: 5px 5px 0px #000;
}
/*Diseño de El titulo de Inicio*/

/*Diseño del Formulario de Inicio y Registro*/
.form{
    transform: translate(-50%, -50%);
    background-color: rgb(88, 29, 143);
    border-radius: 10px;
    position: absolute;
    color: #ffffff;
    width: 300px;    
    border: 1px solid rgb(0, 0, 0);
    text-shadow:
    1px 1px 0px #000;
    font-size: 16px;
    box-shadow: 5px 5px 0px #000;
}
.formreg{
    top: 50%;  
    font-style:normal;
    padding: 30px;
    margin-top: -10px;
    left: 50%;
}
.formlog{
    top: 60%; 
    font-style:normal;
    padding: 30px;
    margin-top: -10px;
    left: 50%; 
}

.titulo{
  margin-top: -80px;

   text-align: center;
